{
  "name": "estupendo",
  "version": "0.0.1",
  "description": "Async npm module loader for the browser",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node /dev/server.js",
    "watch": "watchify src/estupendo.js -o src/estupendo.bundle.js -v",
    "wes": "watchify src/estupendo.js -t babelify -o src/estupendo.bundle.js -v",
    "wor": "watchify src/worker.js -t babelify -o src/worker.bundle.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreasabilio/estupendo.git"
  },
  "keywords": [
    "async",
    "browser",
    "npm",
    "commonjs",
    "loader"
  ],
  "author": "Andreas Abilio helloabilio.dk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreasabilio/estupendo/issues"
  },
  "homepage": "https://github.com/andreasabilio/estupendo#readme",
  "devDependencies": {
    "express": "^4.13.4",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "assign-deep": "^0.4.5",
    "co": "^4.6.0",
    "promise-polyfill": "^5.0.0"
  }
